/* pages/result/result.wxss */
page {
  font-family: "Microsoft YaHei", tahoma, arial, Simsun;min-height: 100%;background:#fbfbfb;
}
.nav {
  /*height: 240rpx;
  border-bottom: 1px #e5e5e5 solid;
  font-family: "Microsoft YaHei", tahoma, arial, Simsun;*/
  width:100%;display: block;
}
.nav image{width:100%;display: block;height: 230rpx;}

.left {
  float: left;
  margin-top: 20rpx;
  margin-left: 8%;
}

.right {
  float: right;
  margin-top: 40rpx;
  margin-right: 8%;
}

.result {
  border-bottom: 1px #e5e5e5 solid;
  overflow: hidden;
}
.table{width:690rpx;border-top:solid 1px #eae9ec;border-left:solid 1px #eae9ec;border-right:solid 1px #eae9ec;margin:40rpx auto 190rpx;font-family: "Microsoft YaHei", tahoma, arial, Simsun;background:#fff;}
.tab{overflow: hidden;background: #f8f8f8;border-bottom: solid 1px #eae9ec;}
.tab text{line-height: 60px;text-align: center;font-size: 26rpx;background: #f8f8f8;float:left;padding:0 9rpx;}
.result text{line-height: 32rpx;text-align: center;font-size: 26rpx;height: 60px;float:left;display: flex;
    justify-content:center;align-items:Center;padding:10rpx 9rpx;}
    .result text:last-child{color:#ffa70f;}
.fanyeprev{width:33%;float:left;height: 98rpx;line-height: 98rpx;text-align: center;background: #3b8ffe;color:#fff;font-size: 28rpx;margin-top:80rpx;position: fixed;left:0;bottom: 0;}
.fanyenum{width:33%;float:left;height: 98rpx;line-height: 98rpx;text-align: center;background: #3b8ffe;color:#fff;font-size: 28rpx;margin-top:80rpx;position: fixed;left:33.5%;bottom: 0;}
.fanyenext{width:33%;float:right;height: 98rpx;line-height: 98rpx;text-align: center;background: #3b8ffe;color:#fff;font-size: 28rpx;margin-top:80rpx;position: fixed;right:0;bottom: 0;}